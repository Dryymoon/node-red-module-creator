{
  "name": "node-red-module-creator",
  "version": "0.0.13",
  "description": "",
  "scripts": {
    "_build": "rm -rf dist/* && webpack -p --config webpack/webpack.js",
    "_dev": "concurrently --kill-others 'npm run _nodemon-watch' 'npm run _webpack-watch'",
    "_nodemon-watch": "nodemon -d 1 --ext js,html -w node_modules/.node-red/nodes --exec 'node-red --userDir ./node_modules/.node-red'",
    "_webpack-watch": "nodemon -w webpack -w .babelrc -w package.json --exec 'webpack --watch --config webpack/webpack.js'"
  },
  "author": "Igor Pilipenko aka Dryymoon",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-catch-binding": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "app-root-dir": "^1.0.2",
    "babel-cli": "^7.0.0-beta.3",
    "babel-loader": "^8.2.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-import-glob": "^2.0.0",
    "concurrently": "^5.3.0",
    "ejs-webpack-loader": "^2.2.2",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "generate-package-json-webpack-plugin": "^2.1.0",
    "glob": "^7.1.6",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "inline-assets-html-plugin": "^1.0.0",
    "interpolate-require-loader": "^1.0.1",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.6",
    "on-build-webpack": "^0.1.0",
    "readline": "^1.3.0",
    "replace-in-file": "^6.1.0",
    "slash": "^3.0.0",
    "uuid": "^8.3.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2",
    "which": "^2.0.2"
  },
  "peerDependencies": {
    "node-red": "*"
  },
  "bin": {
    "node-red-module-create": "./node-red-module-create.js",
    "node-red-module-create-node": "./node-red-module-create-node.js",
    "node-red-module-dev": "./node-red-module-dev.js",
    "node-red-module-build": "./node-red-module-build.js"
  },
  "main": "index.js"
}
