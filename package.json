{
  "name": "node-red-module-creator",
  "version": "0.0.9",
  "description": "",
  "scripts": {
    "build": "rm -rf dist/* && webpack -p --config webpack/webpack.js",
    "dev": "concurrently --kill-others 'npm run _nodemon-watch' 'npm run _webpack-watch'",
    "_nodemon-watch": "nodemon -d 1 --ext js,html -w node_modules/.node-red/nodes --exec 'node-red --userDir ./node_modules/.node-red'",
    "_webpack-watch": "nodemon -w webpack -w .babelrc -w package.json --exec 'webpack --watch --config webpack/webpack.js'"
  },
  "author": "Igor Pilipenko aka Dryymoon",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "app-root-dir": "^1.0.2",
    "babel-cli": "^7.0.0-beta.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-import-glob": "^2.0.0",
    "concurrently": "^4.1.0",
    "ejs": "^2.6.1",
    "ejs-html-loader": "^4.0.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^8.0.1",
    "generate-package-json-webpack-plugin": "^1.0.0",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "no-emit-webpack-plugin": "^2.0.1",
    "nodemon": "^1.19.0",
    "on-build-webpack": "^0.1.0",
    "raw-loader": "^2.0.0",
    "readline": "^1.3.0",
    "replace-in-file": "^4.1.0",
    "slash": "^3.0.0",
    "uuid": "^3.3.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-node-externals": "^1.7.2",
    "which": "^1.3.1"
  },
  "peerDependencies": {
    "node-red": "*"
  },
  "bin": {
    "node-red-module-create": "./node-red-module-create.js",
    "node-red-module-create-node": "./node-red-module-create-node.js",
    "node-red-module-dev": "./node-red-module-dev.js",
    "node-red-module-build": "./node-red-module-build.js"
  },
  "main": "index.js"
}
